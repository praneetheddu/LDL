    "# Perceptron function\n",
    "def compute_output(w, x, target_val=1):\n",
    "    '''\n",
    "    w = weight vector\n",
    "    x = input vector\n",
    "    '''\n",
    "    sum = 0\n",
    "    for i, j in zip(w, x):\n",
    "        sum += (i*j)\n",
    "    return target_val if sum >= 0 else -target_val "
   ]
